{
  "name": "parcel-starter",
  "version": "1.0.1",
  "private": true,
  "description": "A starter template repo for 'vanilla SPAs'",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html"
  },
  "keywords": [
